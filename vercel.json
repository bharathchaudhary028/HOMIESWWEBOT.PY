{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    { "src": "/webhook", "dest": "/api/webhook.py" },
    { "src": "/(.*)", "dest": "/api/webhook.py" }
  ]
}
